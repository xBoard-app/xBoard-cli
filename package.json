    "xBoard-cli": "./bin/run"
  },
  "bugs": "https://github.com/mareksl/xBoard-cli/issues",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "@nestjs/microservices": "^6.11.11",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/listr": "^0.14.2",
    "chalk": "^3.0.0",
    "cli-ux": "^5.4.5",
    "grpc": "^1.24.2",
    "listr": "^0.14.3",
    "rimraf": "^3.0.2",
    "tslib": "^1",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/rimraf": "^2.0.3",
    "@types/yaml": "^1.2.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mareksl/xBoard-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "xBoard-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mareksl/xBoard-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
